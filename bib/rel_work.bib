@article{ariyadasaPhishRepoSeamlessCollection2022,
  title = {{{PhishRepo}}: {{A Seamless Collection}} of {{Phishing Data}} to {{Fill}} a {{Research Gap}} in the {{Phishing Domain}}},
  shorttitle = {{{PhishRepo}}},
  author = {Ariyadasa, Subhash and Fernando, Shantha and Fernando, Subha},
